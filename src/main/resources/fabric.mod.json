{
	"schemaVersion": 1,
	"id": "fastfetch-mc",
	"version": "${version}",
	"name": "Fastfetch MC",
	"description": "Adds the fastfetch command to minecraft",
	"authors": [
		"UntitledHam"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/fastfetch-mc/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"ham.fastfetchmc.FastfetchMC"
		],
		"client": [
			"ham.fastfetchmc.FastfetchMCClient"
		]
	},
	"mixins": [
		"fastfetch-mc.mixins.json",
		{
			"config": "fastfetch-mc.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.3",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}